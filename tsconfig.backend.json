{
  "compilerOptions": {
    "allowJs": false,
    "checkJs": false,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "emitBOM": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "newLine": "lf",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "esnext"
  },
  "files": [
    "config.json"
  ],
  "exclude": [
    "node_modules",
    "static"
  ],
  "include": [
    "**/*"
  ]
}
